{
	"name": "entertainmenthub",
	"version": "0.7.0",
	"author": "Andre Coelho",
	"license": "MIT",
	"main": "app.js",
	"engines": {
		"node": "14.6.0"
	},
	"scripts": {
		"start": "node app.js",
		"server": "nodemon app.js",
		"lint": "eslint '**/*.js' --ignore-pattern node_modules/ --ignore-pattern build/",
		"test": "jest --coverage",
		"client": "cd ../client && npm start",
		"client-install": "cd ../client && npm install",
		"client-build": "cd ../client && npm run build",
		"client-lint": "cd ../client && eslint '**/*.js' --ignore-pattern node_modules/ --ignore-pattern public/",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"deploy": "sh deploy.sh"
	},
	"dependencies": {
		"all-the-cities": "^3.1.0",
		"axios": "^0.21.1",
		"bcryptjs": "^2.4.3",
		"cheerio": "^1.0.0-rc.10",
		"dayjs": "^1.10.6",
		"express": "^4.17.1",
		"mongoose": "^5.13.5",
		"morgan": "^1.10.0",
		"node-cron": "^3.0.0",
		"rss-converter": "^1.1.6",
		"sanitize-html": "^2.4.0",
		"socket.io": "^4.1.3",
		"yahoo-finance2": "^1.14.2"
	},
	"devDependencies": {
		"concurrently": "^6.2.0",
		"eslint": "^7.32.0",
		"jest": "^27.0.6",
		"nodemon": "^2.0.12"
	}
}
